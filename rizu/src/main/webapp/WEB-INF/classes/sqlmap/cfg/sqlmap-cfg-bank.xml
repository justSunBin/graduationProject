<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN"
        "http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="cfgBank">
       <select id="allList"  resultClass="com.renrun.wache.bean.cfg.CfgBank">
              SELECT * FROM cfg_bank

       </select>

       <select id="getById" parameterClass="int"  resultClass="com.renrun.wache.bean.cfg.CfgBank">
              SELECT * FROM cfg_bank WHERE id=#id# limit 1
       </select>


</sqlMap>