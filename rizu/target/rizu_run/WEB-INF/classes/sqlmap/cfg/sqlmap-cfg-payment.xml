<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN"
        "http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="cfgPayment">
       <select id="allList"  resultClass="com.renrun.wache.bean.cfg.CfgPayment">
              SELECT * FROM cfg_payment

       </select>

       <select id="getById" parameterClass="int"  resultClass="com.renrun.wache.bean.cfg.CfgPayment">
              SELECT * FROM cfg_payment WHERE id=#id# limit 1
       </select>

       
</sqlMap>